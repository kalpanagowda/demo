package com;
import java.util.ArrayList;
import java.util.Scanner;

public class CameraRental{
	static Scanner sc=new Scanner(System.in);
	static double Aval_Balance=2500;
	static int cam_id;
	static ArrayList<CameraRental> list=new ArrayList<>();
	int camera_id;//Declaring the variables we needed
	String brand;
	String model;
	double price;
	String status;
	public CameraRental(int camera_id, String brand, String model, double price, String status) {
	super();
	this.camera_id = camera_id;
	this.brand = brand;
	this.model = model;
	this.price = price;
	this.status = status;
	}
